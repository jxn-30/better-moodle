{
    "name": "uzl-better-moodle",
    "description": "Improves the new weird Moodle 4 UI on UzL-Moodle",
    "version": "2.0.0",
    "packageManager": "yarn@4.8.1",
    "type": "module",
    "private": true,
    "scripts": {
        "lint:fix": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --fix",
        "prettier:write": "prettier --write --cache --cache-strategy content .",
        "prettier:dist": "prettier --write dist --ignore-path dummy --print-width 120 --tab-width 2",
        "prettier:i18n": "prettier --write src/i18n",
        "dev": "vite",
        "prebuild": "yarn i18n:once && tsc",
        "build": "yarn prebuild && vite build -- --config=${0} && yarn postbuild",
        "build:uzl": "yarn build uzl",
        "build:cau": "yarn build cau",
        "postbuild": "yarn prettier:dist",
        "preview": "vite preview",
        "i18n:watch": "typesafe-i18n",
        "i18n:once": "typesafe-i18n --no-watch",
        "config:scheme": "ts-json-schema-generator --path 'configs/*.d.ts' --type 'Config' > configs/_schema.json",
        "ics-parser:dev": "wrangler dev --config=ics-parser/wrangler.json --ip=127.0.0.1",
        "ci": "yarn lint:fix && yarn prettier:write && yarn build ${0}"
    },
    "dependencies": {
        "@sass-fairy/map": "1.1.1",
        "@trim21/gm-fetch": "0.3.0",
        "classnames": "2.5.1",
        "darkreader": "4.9.105",
        "jsx-dom": "8.1.6",
        "typesafe-i18n": "5.26.2"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "4.20250402.0",
        "@eslint/js": "9.23.0",
        "@prettier/plugin-xml": "3.4.1",
        "@sass-fairy/string": "1.3.1",
        "@types/eslint": "9.6.1",
        "@types/jquery": "3.5.32",
        "@types/node": "22.13.17",
        "@types/tampermonkey": "5.0.4",
        "@vitejs/plugin-legacy": "6.0.2",
        "browserslist": "4.24.4",
        "browserslist-useragent-regexp": "4.1.3",
        "esbuild": "0.25.2",
        "esbuild-plugin-browserslist": "1.0.0",
        "eslint": "9.23.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-jsdoc": "50.6.9",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-userscripts": "0.5.6",
        "fast-glob": "3.3.3",
        "globals": "16.0.0",
        "jiti": "2.4.2",
        "mini-svg-data-uri": "1.4.4",
        "node-ical": "0.20.1",
        "postcss": "8.5.3",
        "postcss-inline-svg": "6.0.0",
        "postcss-preset-env": "10.1.5",
        "prettier": "3.5.3",
        "rrule": "2.8.1",
        "sass-embedded": "1.86.1",
        "terser": "5.39.0",
        "ts-json-schema-generator": "2.3.0",
        "ts-node": "10.9.2",
        "typescript": "5.8.2",
        "typescript-eslint": "8.29.0",
        "typescript-plugin-css-modules": "5.1.0",
        "userscript-meta": "1.0.1",
        "vite": "6.2.4",
        "vite-plugin-monkey": "5.0.8",
        "wrangler": "4.6.0",
        "yarn": "1.22.22"
    },
    "engines": {
        "node": "22.14.0"
    },
    "browserslist": [
        "last 4 versions",
        "not dead",
        "not Android > 0",
        "not ChromeAndroid > 0",
        "not Safari > 0",
        "not Samsung > 0",
        "not iOS > 0",
        "not op_mob > 0",
        "not UCAndroid > 0",
        "not and_qq > 0",
        "not OperaMini all",
        "not KaiOS > 0"
    ]
}
