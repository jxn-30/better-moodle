{
    "name": "better-moodle",
    "description": "A userscript that improves moodle instances by cool features and design improvements.",
    "version": "2.0.0-rc6",
    "packageManager": "yarn@4.9.2",
    "type": "module",
    "private": true,
    "scripts": {
        "lint:check": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --cache-location .eslintcache --max-warnings 0",
        "lint:fix": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --cache-location .eslintcache --fix",
        "lint:fix:dist": "yarn eslint dist/better-moodle-*.user.js --config=eslint.userscript.config.js --exit-on-fatal-error --cache --cache-strategy content --cache-location .disteslintcache --fix",
        "prettier:check": "prettier --check --cache --cache-strategy content .",
        "prettier:write": "prettier --write --cache --cache-strategy content .",
        "prettier:dist": "prettier --write dist --ignore-path dummy --print-width 120 --tab-width 2",
        "prettier:i18n": "prettier --write src/i18n",
        "dev": "vite",
        "prebuild": "yarn i18n:once && tsc",
        "build": "yarn prebuild && vite build -- --config=${0} && yarn postbuild",
        "build:uzl": "yarn build uzl",
        "build:cau": "yarn build cau",
        "postbuild": "yarn prettier:dist && yarn lint:fix:dist && yarn prettier:dist",
        "preview": "vite preview",
        "test:src": "vitest run --coverage -- --config=${0}",
        "test:userscript": "vitest run --config vitest.userscript.config.ts -- --config=${0}",
        "i18n:watch": "typesafe-i18n",
        "i18n:once": "typesafe-i18n --no-watch",
        "config:scheme": "ts-json-schema-generator --path 'configs/_config.d.ts' --type 'Config' --out configs/_schema.json; ts-json-schema-generator --path 'configs/_global.json.d.ts' --type 'GlobalConfig' --out configs/_globalSchema.json",
        "ics-parser:dev": "wrangler dev --config=ics-parser/wrangler.json --ip=127.0.0.1",
        "ci": "yarn lint:fix && yarn prettier:write && yarn build ${0}"
    },
    "dependencies": {
        "@sass-fairy/map": "1.1.1",
        "@trim21/gm-fetch": "0.3.0",
        "darkreader": "4.9.109",
        "jsx-dom": "8.1.6",
        "typesafe-i18n": "5.26.2"
    },
    "devDependencies": {
        "@actions/core": "1.11.1",
        "@babel/compat-data": "7.28.0",
        "@cloudflare/workers-types": "4.20250828.1",
        "@commitlint/cli": "19.8.1",
        "@commitlint/config-conventional": "19.8.1",
        "@eslint/js": "9.34.0",
        "@prettier/plugin-xml": "3.4.2",
        "@rollup/plugin-terser": "0.4.4",
        "@sass-fairy/string": "1.3.1",
        "@tomjs/unzip-crx": "1.1.3",
        "@types/caniuse-lite": "1.0.5",
        "@types/eslint": "9.6.1",
        "@types/jquery": "3.5.33",
        "@types/mathjax": "0.0.40",
        "@types/node": "24.3.0",
        "@types/tampermonkey": "5.0.4",
        "@vitejs/plugin-legacy": "7.2.1",
        "@vitest/coverage-v8": "3.2.4",
        "babel-plugin-polyfill-corejs3": "0.13.0",
        "boxen": "8.0.1",
        "browserslist": "4.25.3",
        "browserslist-useragent-regexp": "4.1.3",
        "caniuse-lite": "1.0.30001737",
        "chalk": "5.6.0",
        "ci-info": "4.3.0",
        "core-js": "3.45.1",
        "core-js-compat": "3.45.1",
        "esbuild": "0.25.9",
        "esbuild-plugin-browserslist": "1.0.1",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-es-x": "9.1.0",
        "eslint-plugin-jsdoc": "54.1.1",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-userscripts": "0.5.6",
        "fast-glob": "3.3.3",
        "globals": "16.3.0",
        "jiti": "2.5.1",
        "jsdom": "26.1.0",
        "mini-svg-data-uri": "1.4.4",
        "node-ical": "0.20.1",
        "nodejs-file-downloader": "4.13.0",
        "postcss": "8.5.6",
        "postcss-inline-svg": "6.0.0",
        "postcss-preset-env": "10.3.1",
        "prettier": "3.6.2",
        "puppeteer": "24.17.1",
        "rrule": "2.8.1",
        "sass-embedded": "1.91.0",
        "terser": "5.43.1",
        "ts-json-schema-generator": "2.4.0",
        "ts-node": "10.9.2",
        "typescript": "5.9.2",
        "typescript-eslint": "8.41.0",
        "typescript-plugin-css-modules": "5.2.0",
        "userscript-meta": "1.0.1",
        "vite": "7.1.3",
        "vite-plugin-monkey": "7.1.2",
        "vitest": "3.2.4",
        "wrangler": "4.33.1"
    },
    "engines": {
        "node": "^22.18.0"
    },
    "browserslist": [
        "last 4 versions",
        "not dead",
        "not Android > 0",
        "not ChromeAndroid > 0",
        "not Safari > 0",
        "not Samsung > 0",
        "not iOS > 0",
        "not op_mob > 0",
        "not UCAndroid > 0",
        "not and_qq > 0",
        "not OperaMini all",
        "not KaiOS > 0"
    ]
}
