{
    "name": "better-moodle",
    "description": "A userscript that improves moodle instances by cool features and design improvements.",
    "version": "2.5.0",
    "packageManager": "yarn@4.12.0",
    "type": "module",
    "private": true,
    "scripts": {
        "lint:check": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --cache-location .eslintcache --max-warnings 0",
        "lint:fix": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --cache-location .eslintcache --fix",
        "prettier:check": "prettier --check --cache --cache-strategy content .",
        "prettier:write": "prettier --write --cache --cache-strategy content .",
        "prettier:i18n": "prettier --write src/i18n",
        "dev": "vite",
        "prebuild": "yarn i18n:once && tsc",
        "build": "yarn prebuild && vite build -- --config=${0}",
        "release-build": "yarn prebuild && vite build -- --config=${0} --release",
        "build:uzl": "yarn build uzl",
        "build:cau": "yarn build cau",
        "preview": "vite preview",
        "test:src": "vitest run --coverage -- --config=${0}",
        "test:userscript": "vitest run --config vitest.userscript.config.ts -- --config=${0}",
        "i18n:watch": "typesafe-i18n",
        "i18n:once": "typesafe-i18n --no-watch",
        "config:scheme": "ts-json-schema-generator --path 'configs/_config.d.ts' --type 'Config' --out configs/_schema.json; ts-json-schema-generator --path 'configs/_global.json.d.ts' --type 'GlobalConfig' --out configs/_globalSchema.json",
        "ics-parser:dev": "wrangler dev --config=ics-parser/wrangler.json --ip=127.0.0.1",
        "ci": "yarn lint:fix && yarn prettier:write && yarn build ${0}"
    },
    "dependencies": {
        "@sass-fairy/map": "1.1.1",
        "@trim21/gm-fetch": "0.3.0",
        "darkreader": "4.9.117",
        "jsx-dom": "8.1.6",
        "typesafe-i18n": "5.26.2"
    },
    "devDependencies": {
        "@actions/core": "1.11.1",
        "@babel/compat-data": "7.28.5",
        "@cloudflare/workers-types": "4.20251205.0",
        "@commitlint/cli": "20.1.0",
        "@commitlint/config-conventional": "20.0.0",
        "@eslint/js": "9.39.1",
        "@prettier/plugin-xml": "3.4.2",
        "@rollup/plugin-terser": "0.4.4",
        "@sass-fairy/string": "1.3.1",
        "@tomjs/unzip-crx": "1.1.3",
        "@types/caniuse-lite": "1.0.5",
        "@types/eslint": "9.6.1",
        "@types/jquery": "3.5.33",
        "@types/mathjax": "0.0.40",
        "@types/node": "24.10.1",
        "@types/tampermonkey": "5.0.5",
        "@vitejs/plugin-legacy": "7.2.1",
        "@vitest/coverage-v8": "4.0.15",
        "babel-plugin-polyfill-corejs3": "0.13.0",
        "boxen": "8.0.1",
        "browserslist": "4.28.1",
        "browserslist-useragent-regexp": "4.1.3",
        "caniuse-lite": "1.0.30001759",
        "chalk": "5.6.2",
        "ci-info": "4.3.1",
        "core-js": "3.47.0",
        "core-js-compat": "3.47.0",
        "esbuild": "0.27.1",
        "esbuild-plugin-browserslist": "2.0.0",
        "eslint": "9.39.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-es-x": "9.3.0",
        "eslint-plugin-jsdoc": "61.4.1",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-userscripts": "0.5.6",
        "fast-glob": "3.3.3",
        "globals": "16.5.0",
        "jiti": "2.6.1",
        "jsdom": "27.2.0",
        "mini-svg-data-uri": "1.4.4",
        "node-ical": "0.22.1",
        "nodejs-file-downloader": "4.13.0",
        "postcss": "8.5.6",
        "postcss-inline-svg": "6.0.0",
        "postcss-preset-env": "10.5.0",
        "prettier": "3.7.4",
        "puppeteer": "24.32.0",
        "rrule": "2.8.1",
        "sass-embedded": "1.93.3",
        "terser": "5.44.1",
        "ts-json-schema-generator": "2.4.0",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.48.1",
        "typescript-plugin-css-modules": "5.2.0",
        "userscript-meta": "1.0.1",
        "vite": "7.2.6",
        "vite-plugin-monkey": "7.1.5",
        "vitest": "4.0.15",
        "wrangler": "4.53.0"
    },
    "engines": {
        "node": "^24.11.1"
    },
    "browserslist": [
        "last 4 versions",
        "not dead",
        "not Android > 0",
        "not ChromeAndroid > 0",
        "not Safari > 0",
        "not Samsung > 0",
        "not iOS > 0",
        "not op_mob > 0",
        "not UCAndroid > 0",
        "not and_qq > 0",
        "not OperaMini all",
        "not KaiOS > 0"
    ]
}
